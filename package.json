{
  "name": "project_akhir_pemweb",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build-css": "sass Src/Asset/Scss/Style.scss dist/assets/css/Style.css",
    "watch-css": "sass --watch Src/Asset/Scss/Style.scss:dist/assets/css/Style.css",
    "copy-html": "cpx \"Src/index.html\" dist",
    "copy-js": "cpx \"Src/Asset/Js/**\" dist/assets/js",
    "build": "npm run build-css && npm run copy-html && npm run copy-js",
    "format": "prettier --write ."
  },
  "dependencies": {  },
  "devDependencies": {
    "cpx2": "^8.0.0",
    "prettier": "^3.7.4",
    "postcss": "^8.5.6",
    "postcss-cli": "11.0.1",
    "autoprefixer": "10.4.22",
    "sass": "^1.95.1"
  }
}
